<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffc8c8c8" android:endColor="#ffe6e6e6" android:angle="270.0" android:centerColor="#ffe3e3e3" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="@color/color_webview_loading_gradient_start" android:endColor="@color/color_webview_loading_gradient_end" android:angle="0.0" android:centerColor="@color/color_webview_loading_gradient_center" />
            </shape>
        </clip>
    </item>
</layer-list>